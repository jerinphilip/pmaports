+++ b/drivers/devfreq/Makefile
@@ -16,15 +16,15 @@ obj-$(CONFIG_PM_DEVFREQ_EVENT)		+= event/
 
 ifneq ($(CONFIG_MEDIATEK_SOLUTION),)
